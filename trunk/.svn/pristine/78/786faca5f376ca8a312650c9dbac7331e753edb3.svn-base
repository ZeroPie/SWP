    private void AenderungenUebernehmenActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_AenderungenUebernehmenActionPerformed

        //double betrag = (double) this.jFormattedTextFieldBetrag.getValue();
        String vZweck = this.jTextFieldVerwendungszweck.getText();
        int vonKonto  = Integer.valueOf(this.jTextFieldvonKonto.getText());
        Date datum    = (Date) this.jFormattedTextFieldDatum.getValue();
        int kid = ubVGrenz.getKid();
        int uvid = ubVGrenz.getUvid();

        //this.ubVGrenz.setBetrag(betrag);
        this.ubVGrenz.setVonkonto(vonKonto);
        this.ubVGrenz.setDatum(datum);
        this.ubVGrenz.setVerwendungszweck(vZweck);
        this.ubVGrenz.setUvid(kid);
        this.ubVGrenz.setKid(uvid);

        vorlageSteuerung.ueberweisungsvorlageAnlegen(this.ubVGrenz);

        JOptionPane.showMessageDialog(this, "Überweisung erfolgreich bearbeitet", "Erfolg", JOptionPane.INFORMATION_MESSAGE);
        this.dispatchEvent(new WindowEvent(this, WindowEvent.WINDOW_CLOSING));


    }//GEN-LAST:event_AenderungenUebernehmenActionPerformed

