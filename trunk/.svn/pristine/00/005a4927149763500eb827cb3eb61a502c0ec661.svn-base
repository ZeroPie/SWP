    /** Creates new form UeberweisungStornieren */
    public UeberweisungStornierenFrame() {
        initComponents();

        this.presetList = this.vorlageSteuerung.getUeberweisungvorlagenList();

        if(this.presetList != null){
            String[] presetNames = new String[this.presetList.size()];
            System.out.println(this.presetList.size());
            for(int i=0; i < this.presetList.size(); i++){
                System.out.println(this.presetList.get(i).getVonkonto());
                System.out.println(this.presetList.get(i).getVerwendungszweck());

                presetNames[i] = "" + this.presetList.get(i).getVonkonto() + " - " + this.presetList.get(i).getVerwendungszweck();
            }

            DefaultComboBoxModel presetModel = new DefaultComboBoxModel(presetNames);
            this.comboPresets.setModel(presetModel);
        }

    }

